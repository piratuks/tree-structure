{
  "name": "typescript-tree-structure",
  "version": "1.0.2",
  "description": "Versatile and feature-rich TypeScript library with the capability to calculate x and y positions for objects represented in a tree structure while ensuring avoidance of relationship crossings",
  "author": "Evaldas Laureckas",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "cpack": "npm pack",
    "clogin": "npm login",
    "cpublish": "npm publish",
    "test": "mocha --config mocha.config.js",
    "test:nyc": "nyc mocha --config mocha.config.js",
    "test:watch": "mocha --config mocha.config.js --watch",
    "build": "tsc"
  },
  "keywords": [
    "Tree structure",
    "TreeStructure",
    "Position calculation",
    "Coordinate mapping",
    "Relationship avoidance",
    "Hierarchical visualization",
    "Data representation",
    "Graph layout",
    "Crossings elimination",
    "Tree layout algorithm",
    "Node positioning",
    "Visualization toolkit",
    "Data organization",
    "Data structure manipulation",
    "Layout optimization",
    "Tree manipulation",
    "Visualization framework"
  ],
  "bugs": {
    "url": "https://github.com/piratuks/typescript-tree-structure/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/piratuks/typescript-tree-structure.git"
  },
  "license": "MIT",
  "dependencies": {
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.4",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "chai": "^4.3.10",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}